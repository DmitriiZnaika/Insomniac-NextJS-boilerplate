{
  "name": "insomniac",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "dev": "npm run generate && cross-env TS_NODE_PROJECT=tsconfig.server.json node --max_old_space_size=4024 -r ts-node/register server/index.ts",
    "build": "npm run generate && npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "eslint './{src,pages}/**/*' --ext .tsx,.ts",
    "format": "npm run lint -- --fix",
    "pretty": "prettier --write --config .prettierrc.js {src,pages}/**/*.{ts,tsx}",
    "generate": "graphql-codegen --config codegen.yml",
    "analyze": "cross-env ANALYZE=true npm run build",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "next": "10.0.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.2",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@next/bundle-analyzer": "^10.0.6",
    "@storybook/addon-actions": "^6.1.16",
    "@storybook/addon-docs": "^6.1.16",
    "@storybook/addon-knobs": "^6.1.16",
    "@storybook/addon-links": "6.1.16",
    "@storybook/addon-storyshots": "^6.1.16",
    "@storybook/addon-storysource": "^6.1.16",
    "@storybook/addon-viewport": "^6.1.16",
    "@storybook/react": "6.1.16",
    "@storybook/source-loader": "^6.1.16",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.25",
    "@types/react": "^16.9.49",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-react-app": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
